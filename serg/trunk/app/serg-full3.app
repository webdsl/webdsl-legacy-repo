  }

  TechnicalReport : Publication {
    number :: Int ( )
    document :: Text ( )
    preprintof -> Publication ( )
  }

  InProceedings : Publication {
    conference -> Conference ( )
    pages :: String ( )
  }

  Conference {
    fullname :: String ( )
    acronym :: String ( name )
    booktitle :: String ( )
    editors -> List<Person> ( )
    place :: String ( )
    year :: Int ( )
    month :: String ( )
    url :: URL ( )
    acceptance :: Int ( )
