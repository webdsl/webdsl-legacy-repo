module sdf2imp/services/create-main-descriptor

imports
  libstratego-lib
  
  sdf2imp/util/-

strategies

  main-descriptor-name = <descriptor-name> "-Main"

  create-main-descriptor =
    <output-default-descriptor-file>
      |[ module <main-descriptor-name>
         
        imports <descriptor-name> "-Colorer",
                <descriptor-name> "-Outliner",
                <descriptor-name> "-Folding"
      ]|
