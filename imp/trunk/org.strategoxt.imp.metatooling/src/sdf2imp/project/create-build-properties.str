module sdf2imp/project/create-build-properties

imports
  sdf2imp/util/-

strategies

  create-build-properties =
    esv   := <packed-descriptor-file-name>;
    table := <parse-table-target>;
    
    <output-text-file(|[], "build.properties")>
    
<conc-strings> (
"source.. = src/
output.. = bin/
bin.includes = META-INF/,\\
               .,\\
               plugin.xml,\\
               ", table, ",\\
               include/", esv, "\\
               bin/,\\
               /
")
