module performance-tweaks

signature constructors
  Cons   : Term * List -> List
  Nil    : List
  Stream : Int -> Stream

strategies

  map(s) = (?[_ | _] + ?[]); all(s)
  
  getfirst(s) = (?[_ | _] + ?[]); one(s; ?x); !x
  
  read-text-from-stream =
    ?Stream(stream);
    prim("SSL_EXT_read_text_from_stream", stream)
  