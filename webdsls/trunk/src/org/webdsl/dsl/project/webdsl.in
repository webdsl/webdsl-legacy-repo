#!@bash@
export prefix=`dirname "$(cd ${0%/*}/.. && echo $PWD/${0##*/})"`

if test -z "$ANT_LOGGER"; then
  ANT_LOGGER="org.webdsl.ant.WebDSLAntLogger"
fi

ant -f "${prefix}/share/webdsl/webdsl-build.xml" -Dtemplatedir="${prefix}/share/webdsl" -Dcurrentdir="`pwd`" -Dwebdsl-java-cp="${prefix}/bin/webdsl.jar" -Dwebdslexec="@WEBDSLC@" -Dbuildoptions="$*" command -lib "${prefix}/share/webdsl/template-java-servlet/lib/webdsl-support.jar" -logger $ANT_LOGGER
