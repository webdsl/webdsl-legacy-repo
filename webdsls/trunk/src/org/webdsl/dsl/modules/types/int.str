module org/webdsl/dsl/modules/types/int

imports
  libstrategolib
  libwebdsl-front

rules
  is-simple-sort = ?SimpleSort("Int")
  extends-builtin : "Int" -> "Object"

rules // type

  type-default-value :
    SimpleSort("Int") -> exp|[ 0 ]|

  eq-type-of :
    Int(x) -> SimpleSort("Int")

rules // conversion

 check-builtin-signature :
    (SimpleSort("Int"), "floatValue", []) ->  SimpleSort("Float")