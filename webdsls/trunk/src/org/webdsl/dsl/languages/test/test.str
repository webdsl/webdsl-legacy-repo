module org/webdsl/dsl/languages/test/test

imports
  libstrategolib

imports
  libwebdsl-front

rules

  rename-test :
    t@Test(ident,b1) -> Test(ident{newident},b2)
    with  newident := <newname> ident
        ; {| Rename, InDef
          : rules ( InDef := True() )
          ; b2 := <rename-all> b1
          |}
        
  get-toplevel-name = ?Test(<id>, _)
	