module org/webdsl/dsl/languages/ajax/normalize

rules //normalize Ajax
  normalize-ajax :
    AjaxActionIdParam(a, d, v) -> AjaxAction(a, d, String(v))
    
  normalize-ajax : 
    AjaxActionNoParam(a, d) -> AjaxAction(a, d, String(""))
    
  normalize-ajax :
    AjaxActionNoTarget(a, v) -> AjaxAction(a, "this", v)
