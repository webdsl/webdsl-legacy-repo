module org/webdsl/dsl/languages/hql/typechecker

rules

  type-of-query :
    QueryRule(
      SelectFrom(_, FromClause(ranges)), _, _, _) -> GenericSort("List", [t])
    where t := <fetch-elem(get-type-from-range)> ranges

  get-type-from-range :
    FromRangeJoin(FromClass(Path([x_Class]),Some(AsAlias(Alias(_))),_),_) -> SimpleSort(x_Class)
