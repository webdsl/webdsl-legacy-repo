module org/webdsl/dsl/languages/ui/page-not-found

imports
  libstrategolib
  libwebdsl-front
    
rules        
       
  desugar-ui :
    a@Application(_,_) -> a
    where not(get-page-not-found-def)
    with  default-page-not-found-def
  
  get-page-not-found-def = !<TemplateDefinition> "pagenotfound"

  default-page-not-found-def =
    <emit-webdsl-code> def|[
      define ignore-access-control page pagenotfound(){
        <h3>"404 Not Found"</h3>
      } 
    ]|
    
