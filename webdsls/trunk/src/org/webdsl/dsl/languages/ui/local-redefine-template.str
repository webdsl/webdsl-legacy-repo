module org/webdsl/dsl/languages/ui/local-redefine-template

imports 
  libstrategolib
  
imports 
  libwebdsl-front

rules

  constraint-error-ui :
    LocalRedefine(mods,tname,fargs,tname2) -> <add-error(|["Local template redefinition requires a template with signature: ",ppcall])>
    where not(<get-exact-definition-from-sig> (tname2, fargs))
    with  ppcall := <pp-func-sig> (tname2,fargs)
