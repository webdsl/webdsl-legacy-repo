module org/webdsl/dsl/languages/webservice-synchronization/declare



imports
  libwebdsl-front
  org/webdsl/dsl/generation/webdsl-generator

strategies
	
  declare-synchronization-settings(|ent) = declare-synchronization-topentity(|ent)


  declare-synchronization(|ent) : x@WebServiceMappingEmbedded(content) -> x
  	where <filter(declare-synchronization-settings)> content
  

  declare-synchronization-topentity(|ent): x@WebServiceTopLevelNameProperty(propertyname) -> x
    with rules (
    	TopEntity :+ ent -> propertyname
    )