module org/webdsl/dsl/to-java-servlet/ajax/placeholder
   
imports
  libstrategolib  
  libwebdsl-front

rules

  elem-to-java-servlet :
    Placeholder(x_name, elems) ->
    bstm* |[ 
    out.print("<span id=\""+"~x_name"+"\" class=\"webdsl-placeholder\">");
    bstm*
    out.print("</span>");
    ]|
    with bstm* := <elems-to-java-servlet> elems 
  