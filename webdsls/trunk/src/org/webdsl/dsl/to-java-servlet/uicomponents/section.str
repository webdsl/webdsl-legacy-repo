module org/webdsl/dsl/to-java-servlet/uicomponents/section

imports
  libstrategolib

imports
  libwebdsl-front
  
rules 

  elem-to-java-servlet :
    TemplateCall("section", [],passign*, elems) ->
    bstm* |[ 
      threadLocalPageCached.enterSectionContext();
      out.print("<span class=\"section section"+threadLocalPageCached.getSectionDepth()+e_attrs2+"\" "+e_attrs1+">");
      bstm*
      out.print("</span>");
      threadLocalPageCached.leaveSectionContext();
    ]|
    with  bstm* := <elems-to-java-servlet> elems
        ; (e_attrs1, e_attrs2) := <to-html-attrs-separate-class> passign*
