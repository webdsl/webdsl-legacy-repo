/**

 Desugaring rules for WebDSL
 
*/

module org/webdsl/dsl/generation/desugar

imports 
  libstrategolib 
  Java-15 
  libjava-front 
  
imports 
  org/webdsl/dsl/syntax/WebDSL

strategies

  desugar = 
    topdown(try(Desugar))

rules

  Desugar : 
    PropertySimpleNoAnno(x, s) -> PropertySimple(x, s, [])
    
  Desugar : 
    PropertyCompNoAnno(x, s) -> PropertyComp(x, s, [])
    
  Desugar : 
    PropertyRefNoAnno(x, s) -> PropertyRef(x, s, [])
