module Stratego-WebDSL-Python

imports
  StrategoWebDSL
  MixPython[PythonTerm]

hiddens
  context-free start-symbols Module[[StrategoHost]] 

exports
  
  context-free syntax
    "python" "|[" Statement[[PythonTerm]] "]|"  -> Term [[StrategoHost]] {cons("ToMetaExpr")}

  variables
    "e" [0-9]*     -> Exp[[PythonTerm]]  {prefer}

