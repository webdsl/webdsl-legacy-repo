module primitives

rules // primitive types

declare-primitive-types =
  where( primitive-types; map(declare-primitive-type) )
  
primitive-types =
  !["String", "Int", "Text", "WikiText", "Secret"]
  
declare-primitive-type =
  ?x; rules( IsPrimitiveType : x -> x )
  
is-primitive-type =
   SimpleType(IsPrimitiveType)
   
is-string-type =
  ?SimpleType("String")
