module org/webdsl/dsl/to-java-servlet/uicomponents/div

imports
  libstratego-lib
  libjava-front

imports
  libwebdsl-front
    
rules

  // @obsolete; use block{ ... } instead of div{ ... }
  elem-to-java-servlet :
    TemplateCall("div", [String(x)], elems) ->
    bstm* |[ 
    out.print("<div>");
    bstm*
    out.print("</div>");
    ]|
    with  bstm* := <elems-to-java-servlet> elems 
