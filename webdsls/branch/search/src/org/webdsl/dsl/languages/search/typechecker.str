module org/webdsl/dsl/languages/search/typechecker

imports
  libwebdsl-front
  org/webdsl/dsl/languages/data-model/declare

rules

  type-exists-search=
	?SearchSort(sort);where(<is-entity-type> sort)

  type-default-value :
    SearchSort(_) -> exp|[ null ]|
    
  type-requires-default-anno = ?SearchSort(_)