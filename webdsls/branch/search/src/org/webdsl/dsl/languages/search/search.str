module org/webdsl/dsl/languages/search/search

imports
  libstrategolib
  libjava-front

imports
  libwebdsl-front
  
  
rules
	is-searchable-prop = where(?DerivedProperty(_, _, _, anno*, _) <+ ?Property(_, _, _, anno*));!anno*; where(has-searchable-anno)
