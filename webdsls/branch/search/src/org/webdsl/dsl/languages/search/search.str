module org/webdsl/dsl/languages/search/search

imports
  libstrategolib
  libjava-front

imports
  libwebdsl-front
  
  
rules
	is-searchable-prop = where(?p@DerivedProperty(_, _, _, anno*, _) <+ ?p@Property(_, _, _, anno*));!anno*; where(has-searchable-anno); !p
	has-searchable-anno = fetch(?SearchableAnno(_) <+ ?SimpleAnno("searchable") <+ ?SearchableAnno())