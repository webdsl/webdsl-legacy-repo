module org/webdsl/dsl/analysis/search

imports
  libstrategolib
  libjava-front

imports
  libwebdsl-front

strategies
  
  // search configuration
  analyze :
  	s@SearchConfig(name, body) -> s
  	with if(<uses-NGram-filter> body) then rules (UsesNGramFilter: name -> []) end

  uses-NGram-filter:
  	s@SearchConfigBody(_, _, tokenFilter*) -> s
  	where(<fetch(?TokenFilter("NGramFilterFactory", _))> tokenFilter*)