module pretty-print

// parses a NanoWebDSL file and pretty-prints it

imports
  libstratego-lib
  libstratego-xtc
  libwebdsl-front

strategies

  main =
    xtc-io-wrap(
        webdslc-options
      , webdslc-usage
      , webdslc-about
      , ![] // xtc dependencies
      , xtc-pretty-print
    )

  xtc-pretty-print = id
    ; set-appname
    ; set-default-config
    ; xtc-parse-webdsl
    ; output-webdsl
